{
    "publisher": "MartinnDev",
    "name": "vscset",
    "displayName": "BakkesMod Plugin Set file",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages", "Snippets"
    ],
    "repository": {
        "type" : "git",
        "url" : "https://github.com/Martinii89/VSCodeBM-Set"
      },
    "contributes": {
        "languages": [{
            "id": "set",
            "aliases": ["BM Plugin set", "set"],
            "extensions": [".set"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "set",
            "scopeName": "bakkesmod.plugin.set",
            "path": "./syntaxes/set.tmLanguage.json"
        }],
        "snippets": [
            {
              "language": "set",
              "path": "./snippets/snippets.json"
            }
          ]
    }
}